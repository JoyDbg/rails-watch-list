<%= render "banner", list: @list %>

<div class="container">
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.input :image_url %>
    <%= f.button :submit, class: "btn btn-flat" %>
  <% end %>
  <%= link_to "Back to list", lists_path , class: "btn btn-flat" %>
</div>
<%= render "footer", list: @list %>
